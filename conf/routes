# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET    /                           	controllers.Application.index(q: String)
POST   /enonce/:id                	controllers.Application.enonce(id: Int)
GET    /scalaskel/change/:money		controllers.Application.scalaskel(money: Int)
POST   /jajascript/optimize			controllers.Application.jajascript()
POST   /minesweeper/resolve			controllers.Application.minesweeper()
POST   /diet/resolve                controllers.Application.diet()

